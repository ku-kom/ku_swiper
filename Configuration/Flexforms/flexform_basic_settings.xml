		<type>array</type>
        <el>
            <settings.autoplay>
                <TCEforms>
                    <label>LLL:EXT:ku_swiper/Resources/Private/Language/locallang_be.xlf:autoplay
                    </label>
                    <exclude>1</exclude>
                    <config>
                        <type>check</type>
